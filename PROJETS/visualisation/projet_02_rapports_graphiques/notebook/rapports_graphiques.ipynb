    "        <div class=\"value\">{{ metric.valeur }}</div>\n",
    "        <div class=\"label\">{{ metric.nom }}</div>\n",
    "    </div>\n",
    "    {% endfor %}\n",
    "    \n",
    "    <h2>Donn√©es</h2>\n",
    "    <table border=\"1\" cellpadding=\"10\">\n",
    "        <tr>\n",
    "            {% for col in colonnes %}\n",
    "            <th>{{ col }}</th>\n",
    "            {% endfor %}\n",
    "        </tr>\n",
    "        {% for row in donnees %}\n",
    "        <tr>\n",
    "            {% for col in colonnes %}\n",
    "            <td>{{ row[col] }}</td>\n",
    "            {% endfor %}\n",
    "        </tr>\n",
    "        {% endfor %}\n",
    "    </table>\n",
    "</body>\n",
    "</html>\n",
    "\"\"\"\n",
    "\n",
    "# Pr√©parer les donn√©es\n",
    "context = {\n",
    "    'titre': 'Rapport Automatis√©',\n",
    "    'date': datetime.now().strftime('%d/%m/%Y %H:%M'),\n",
