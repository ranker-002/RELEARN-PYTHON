    "## 2. G√©n√©ration de Graphiques pour Rapports\n",
    "\n",
    "```python\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from pathlib import Path\n",
    "\n",
    "# Cr√©er le dossier de sortie\n",
    "OUTPUT_DIR = Path(\"../../data/rapports\")\n",
    "OUTPUT_DIR.mkdir(parents=True, exist_ok=True)\n",
    "\n",
    "# G√©n√©rer des donn√©es\n",
    "np.random.seed(42)\n",
    "df = pd.DataFrame({\n",
    "    'date': pd.date_range(start='2024-01-01', periods=30, freq='D'),\n",
    "    'ventes': np.random.randint(5000, 15000, 30),\n",
    "    'visiteurs': np.random.randint(1000, 5000, 30),\n",
    "    'conversion': np.random.uniform(2, 8, 30)\n",
    "})\n",
    "\n",
    "def generer_graphique_ventes(df, output_path):\n",
    "    \"\"\"G√©n√®re un graphique de ventes pour le rapport.\"\"\"\n",
    "    fig, axes = plt.subplots(2, 1, figsize=(10, 8))\n",
    "    \n",
    "    # Graphique principal\n",
    "    axes[0].plot(df['date'], df['ventes'], marker='o', linewidth=2, color='#2E86AB')\n",
    "    axes[0].fill_between(df['date'], df['ventes'], alpha=0.3, color='#2E86AB')\n",
    "    axes[0].set_title('√âvolution des Ventes - Janvier 2024', fontsize=14, fontweight='bold')\n",
    "    axes[0].set_ylabel('Ventes (‚Ç¨)')\n",
    "    axes[0].grid(True, alpha=0.3)\n",
    "    \n",
    "    # M√©triques cl√©s\n",
    "    total = df['ventes'].sum()\n",
    "    moyenne = df['ventes'].mean()\n",
    "    max_val = df['ventes'].max()\n",
    "    \n",
    "    metrics_text = f\"\"\"M√âTRIQUES CL√âS\n",
    "    Total: {total:,}‚Ç¨\n",
    "    Moyenne: {moyenne:,.0f}‚Ç¨\n",
    "    Maximum: {max_val:,}‚Ç¨\"\n",
    "    \n",
    "    axes[1].text(0.05, 0.5, metrics_text, transform=axes[1].transAxes,\n",
    "                 fontsize=12, verticalalignment='center',\n",
    "                 bbox=dict(boxstyle='round', facecolor='lightblue', alpha=0.5))\n",
    "    axes[1].axis('off')\n",
    "    \n",
    "    plt.tight_layout()\n",
    "    plt.savefig(output_path, dpi=150, bbox_inches='tight')\n",
    "    plt.close()\n",
    "    \n",
    "    return str(output_path)\n",
    "\n",
    "# G√©n√©rer\n",
    "graph_path = generer_graphique_ventes(df, OUTPUT_DIR / \"graphique_ventes.png\")\n",
    "print(f\"‚úÖ Graphique g√©n√©r√©: {graph_path}\")\n",
    "```\n",
    "\n",
    "---"
   ]
