    "## 6. Export Multi-Format\n",
    "\n",
    "```python\n",
    "from pathlib import Path\n",
    "import json\n",
    "\n",
    "def exporter_donnees(dataframe, base_path, formats=['csv', 'json', 'excel']):\n",
    "    \"\"\"Exporte les donn√©es dans plusieurs formats.\"\"\"\n",
    "    \n",
    "    base_path = Path(base_path)\n",
    "    base_path.mkdir(parents=True, exist_ok=True)\n",
    "    \n",
    "    resultats = {}\n",
    "    \n",
    "    for fmt in formats:\n",
    "        if fmt == 'csv':\n",
    "            path = base_path / \"donnees.csv\"\n",
    "            dataframe.to_csv(path, index=False)\n",
    "            resultats['CSV'] = path\n",
    "        \n",
    "        elif fmt == 'json':\n",
    "            path = base_path / \"donnees.json\"\n",
    "            dataframe.to_json(path, orient='records', date_format='iso')\n",
    "            resultats['JSON'] = path\n",
    "        \n",
    "        elif fmt == 'excel':\n",
    "            # !pip install openpyxl\n",
    "            path = base_path / \"donnees.xlsx\"\n",
    "            with pd.ExcelWriter(path, engine='openpyxl') as writer:\n",
    "                dataframe.to_excel(writer, sheet_name='Donn√©es', index=False)\n",
    "            resultats['Excel'] = path\n",
    "    \n",
    "    return resultats\n",
    "\n",
    "# Exporter\n",
