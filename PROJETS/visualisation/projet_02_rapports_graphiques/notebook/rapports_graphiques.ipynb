    "## 5. Syst√®me d'Automatisation\n",
    "\n",
    "```python\n",
    "import schedule\n",
    "import time\n",
    "from datetime import datetime\n",
    "\n",
    "class GenerateurRapports:\n",
    "    \"\"\"Syst√®me automatis√© de g√©n√©ration de rapports.\"\"\"\n",
    "    \n",
    "    def __init__(self, output_dir):\n",
    "        self.output_dir = Path(output_dir)\n",
    "        self.output_dir.mkdir(parents=True, exist_ok=True)\n",
    "    \n",
    "    def collecter_donnees(self):\n",
    "        \"\"\"R√©cup√®re les donn√©es √† analyser.\"\"\"\n",
    "        # Simulation - en production, connexion BDD/API\n",
    "        return df\n",
    "    \n",
    "    def analyser(self, data):\n",
    "        \"\"\"Effectue l'analyse des donn√©es.\"\"\"\n",
    "        return {\n",
    "            'total_ventes': data['ventes'].sum(),\n",
    "            'moyenne_ventes': data['ventes'].mean(),\n",
    "            'total_visiteurs': data['visiteurs'].sum(),\n",
    "            'conversion': data['conversion'].mean()\n",
    "        }\n",
    "    \n",
    "    def generer_rapports(self, data, metrics):\n",
    "        \"\"\"G√©n√®re tous les formats de rapports.\"\"\"\n",
    "        timestamp = datetime.now().strftime(\"%Y%m%d_%H%M%S\")\n",
    "        \n",
    "        # Graphique\n",
    "        graph_path = self.output_dir / f\"graphique_{timestamp}.png\"\n",
    "        generer_graphique_ventes(data, graph_path)\n",
    "        \n",
    "        # HTML\n",
    "        html_path = self.output_dir / f\"rapport_{timestamp}.html\"\n",
    "        generer_rapport_html({'titre': f'Rapport - {timestamp}'}, \n",
    "                            f\"graphique_{timestamp}.png\", html_path)\n",
    "        \n",
    "        # PDF\n",
    "        pdf_path = self.output_dir / f\"rapport_{timestamp}.pdf\"\n",
    "        generer_rapport_pdf({'titre': f'Rapport - {timestamp}'}, data, pdf_path)\n",
    "        \n",
    "        return {'html': html_path, 'pdf': pdf_path, 'graph': graph_path}\n",
    "    \n",
    "    def executer_quotidien(self):\n",
    "        \"\"\"Ex√©cution quotidienne du pipeline.\"\"\"\n",
    "        print(f\"\\n{'='*50}\")\n",
    "        print(f\"üïê G√©n√©ration du rapport - {datetime.now()}\")\n",
    "        print(f\"{'='*50}\")\n",
    "        \n",
    "        data = self.collecter_donnees()\n",
    "        metrics = self.analyser(data)\n",
    "        fichiers = self.generer_rapports(data, metrics)\n",
    "        \n",
    "        print(f\"\\n‚úÖ Rapports g√©n√©r√©s:\")\n",
    "        for fmt, path in fichiers.items():\n",
    "            print(f\"   {fmt.upper()}: {path}\")\n",
    "    \n",
    "    def planifier(self, heure='08:00'):\n",
    "        \"\"\"Planifie la g√©n√©ration quotidienne.\"\"\"\n",
    "        schedule.every().day.at(heure).do(self.executer_quotidien)\n",
    "        print(f\"üìÖ Rapport quotidien planifi√© √† {heure}\")\n",
    "        \n",
    "        while True:\n",
    "            schedule.run_pending()\n",
    "            time.sleep(60)\n",
    "\n",
    "# Utilisation\n",
    "generateur = GenerateurRapports(\"../../data/rapports\")\n",
    "\n",
    "# Ex√©cution unique (commenter pour scheduling)\n",
    "# generateur.executer_quotidien()\n",
    "\n",
    "# Pour planification continue\n",
    "# generateur.planifier(\"08:00\")\n",
    "```\n",
    "\n",
